  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "listr2": "^8.2.5",
    "lodash": "^4.17.21",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/semver": "^7.5.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1"
